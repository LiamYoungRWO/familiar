Package: familiar
Title: End-To-End Machine Learning and Data Analysis Framework
Version: 0.0.0.54
Authors@R: c(
   person("Alex", "Zwanenburg",
          email = "alexander.zwanenburg@nct-dresden.de",
          role = c("aut", "cre"),
          comment = c(ORCID = "0000-0002-0342-9545")),
   person("Steffen", "LÃ¶ck", role="aut"),
